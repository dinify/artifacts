{
  "palette": {
    "type": "light",
    "primary": {
      "light": "#E14846",
      "main": "#C13939",
      "dark": "#B1312F"
    },
    "text": {
      "primary": "rgba(0, 0, 0, 0.72)",
      "secondary": "rgba(0, 0, 0, 0.38)"
    },
    "background": {
      "paper": "#ffffff",
      "default": "#fafafa"
    }
  },
  "shape": {
    "borderRadius": 8
  },
  "typography": {
    "useNextVariants": true,
    "fontFamily": [
      "Lato",
      "-apple-system",
      "BlinkMacSystemFont",
      "\"Segoe UI\"",
      "Roboto",
      "\"Helvetica Neue\"",
      "Arial",
      "sans-serif",
      "\"Apple Color Emoji\"",
      "\"Segoe UI Emoji\"",
      "\"Segoe UI Symbol\""
    ]
  },
  "overrides": {
    "MuiInputBase": {
      "root": {
        "& > input:-webkit-autofill, & > input:-webkit-autofill:hover, & > input:-webkit-autofill:focus": {
          "WebkitTextFillColor": "rgba(0, 0, 0, 0.72)",
          "WebkitBoxShadow": "0 0 0px 1000px rgba(0,0,0,0) inset !important",
          "WebkitTransitionDelay": "99999s",
          "caretColor": "rgba(0, 0, 0, 0.72)"
        }
      }
    },
    "MuiInputLabel": {
      "filled": {
        "transform": "translate(12px, 18px) scale(1.333)",
        "&$shrink": {
          "transform": "translate(12px, 10px) scale(1)",
          "&$marginDense": {
            "transform": "translate(12px, 7px) scale(1)"
          }
        }
      }
    },
    "MuiFilledInput": {
      "root": {
        "borderRadius": 8,
        "overflow": "hidden"
      },
      "underline": {
        "&:before": {
          "borderBottom": "none"
        },
        "&:hover:not($disabled):not($focused):not($error):before": {
          "borderBottom": "none"
        }
      }
    }
  }
}